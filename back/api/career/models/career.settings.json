{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "careers",
  "info": {
    "name": "Career"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "begin": {
      "type": "date"
    },
    "end": {
      "type": "date"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "careers"
    },
    "thumbnail": {
      "type": "string"
    }
  }
}
