{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "tags",
  "info": {
    "name": "Tag"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "tags",
      "dominant": true
    },
    "name": {
      "type": "string"
    },
    "articles": {
      "collection": "article",
      "via": "tags"
    },
    "color": {
      "type": "string",
      "default": "#1890ff"
    }
  }
}
