{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "Article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "tags": {
      "via": "articles",
      "collection": "tag",
      "dominant": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "articles"
    },
    "category": {
      "model": "category",
      "via": "articles"
    },
    "thumbnail": {
      "type": "string"
    },
    "contents": {
      "type": "richtext"
    },
    "description": {
      "type": "string"
    },
    "width": {
      "type": "integer",
      "default": 300,
      "max": 700
    }
  }
}
